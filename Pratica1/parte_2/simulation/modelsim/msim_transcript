add wave -position 0 -autoscale 1 -format Literal -height 17 -editable 2 Edit:/MemoriaRAM/address
wave create -driver freeze -pattern counter -startvalue 00000000 -endvalue 11111111 -type Range -direction Up -period 100ps -step 1 -repeat forever -range 7 0 -starttime 0ps -endtime 1000ps sim:/MemoriaRAM/address
add wave -position 0 -autoscale 1 -format Literal -height 17 -editable 2 Edit:/MemoriaRAM/address
force -freeze sim:/MemoriaRAM/clock 0 0, 1 {50 ps} -r 100
force -freeze sim:/MemoriaRAM/wren 0 0
